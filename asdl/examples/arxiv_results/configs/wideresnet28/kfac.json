{
    "dataset": "CIFAR10",
    "model":"wideresnet28",
    "data_augmentation":true,
    "auto_augment":true,
    "cutout":true,
    "n_holes":1,
    "length":16,
    "epochs": 100,
    "batch_size": 512,
    "train_size":45056,
    "weight_decay":5e-4,
    "clipping_norm":10,
    "optim":"kfac_mc",
    "lr":0.3,
    "momentum":0.9,
    "damping":1e-3,
    "curvature_update_interval":10,
    "ema_decay":-1
}